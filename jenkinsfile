pipeline {
  agent none
  stages {
    stage('Back-end') {
      steps {
        sh 'echo "Hello this is backend"'
      }
    }

    stage('Front-end') {
      agent {
        docker {
          image 'node:16-alpine'
        }

      }
      steps {
        sh 'node --version'
      }
    }

  }
}